<Window x:Class="EventDriven_Dashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EventDriven_Dashboard"
        mc:Ignorable="d"
        Title="MainWindow" Height="540" Width="960" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel x:Name="Navigation" HorizontalAlignment="Left" Width="74">
            <Button x:Name="HomeBTN" Content="Home" Click="HomeBTN_Click"></Button>
            <Button x:Name="ToDoBTN" Content="To-Do List" Click="ToDoBTN_Click"></Button>
            <Button x:Name="NotesBTN" Content="Notes" Click="NotesBTN_Click"></Button>
            <Button x:Name="ResoucesBTN" Content="Resources" Click="ResoucesBTN_Click"></Button>
            <Button x:Name="MediaBTN" Content="Media" Click="MediaBTN_Click"></Button>
        </StackPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="74,0,0,0">
        </Frame>
    </Grid>
</Window>
